<!doctype html>
<html lang="en">
  <head>
    <title>AiMu Dental Clinic</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,700,900|Rubik:300,400,700" rel="stylesheet">

    <link rel="stylesheet" href="css/bootstrap.css">
    <link rel="stylesheet" href="css/animate.css">
    <link rel="stylesheet" href="css/owl.carousel.min.css">

    <link rel="stylesheet" href="fonts/ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="fonts/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/magnific-popup.css">

    
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

    <!-- Theme Style -->
    <link rel="stylesheet" href="css/style.css">
  </head>
  <body>
    
    <header role="banner">
     
      <nav class="navbar navbar-expand-md navbar-dark bg-light">
        <div class="container">
          <a class="navbar-brand" href="/">AiMu Dental Clinic</a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExample05" aria-controls="navbarsExample05" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>

          <div class="collapse navbar-collapse navbar-light" id="navbarsExample05">
            <ul class="navbar-nav ml-auto pl-lg-5 pl-0">
              <li class="nav-item">
                <a class="nav-link active" href="/adminPage">Home</a>
                <a class="nav-link active" href="/listBooking">Back</a>
              </li>
            </ul>
            
          </div>
        </div>
      </nav>
    </header>
    <!-- END header -->


  <section class="site-section">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <h2 class="mb-5">Patient Record</h2>
          <div class="table-content table-responsive">
            <table class="w3-table-all w3-centered">
                <thead>
                    <tr>
                        <th class="product-name">Patient Name</th>
                        <th class="product-edit">Date</th>
                        <th class="product-edit">Service</th>
                        <th class="product-edit">Doctor</th>
                        <th class="product-quantity">Status</th>
                    </tr>
                </thead>
                <tbody>
                  <% for(var i = 0; i < data.length; i++) { %>
                    <tr>
                      <td><%= data[i].user_name %></td>
                      <td><%= data[i].bookDate %></td>                   
                      <td><%= data[i].services %></td>
                      <td><%= data[i].doctor %></td>
                      <td><%= data[i].status %></td>
                    </tr>
                      <% } %>
                   
                    </tbody>
                </table>
            </div>
        </div>
      </div>
    </div>
  </section>
  <!-- END section -->

  <!-- END section -->

  <script src="js/main.js"></script>
</body>
</html>